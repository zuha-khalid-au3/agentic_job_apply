# LocalAI Configuration (Free Alternative)
USE_LOCALAI=true
LOCALAI_URL=http://localhost:8080
LOCALAI_MODEL=gpt-3.5-turbo

# Fallback provider if LocalAI is not available
FALLBACK_PROVIDER=openai

# OpenAI Configuration (Original)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ENDPOINT=https://api.openai.com/v1

# Google Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Mistral Configuration
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_ENDPOINT=https://api.mistral.ai/v1

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_API_VERSION=2025-01-01-preview

# Ollama Configuration
OLLAMA_ENDPOINT=http://localhost:11434

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_ENDPOINT=https://api.deepseek.com

# Grok Configuration
GROK_API_KEY=your_grok_api_key_here
GROK_ENDPOINT=https://api.x.ai/v1

# IBM Configuration
IBM_API_KEY=your_ibm_api_key_here
IBM_PROJECT_ID=your_ibm_project_id_here
IBM_ENDPOINT=https://us-south.ml.cloud.ibm.com

# Alibaba Configuration
ALIBABA_API_KEY=your_alibaba_api_key_here
ALIBABA_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1

# Moonshot Configuration
MOONSHOT_API_KEY=your_moonshot_api_key_here
MOONSHOT_ENDPOINT=https://api.moonshot.cn/v1

# Unbound Configuration
UNBOUND_API_KEY=your_unbound_api_key_here
UNBOUND_ENDPOINT=https://api.getunbound.ai

# SiliconFlow Configuration
SILICONFLOW_API_KEY=your_siliconflow_api_key_here
SILICONFLOW_ENDPOINT=https://api.siliconflow.cn/v1

# ModelScope Configuration
MODELSCOPE_API_KEY=your_modelscope_api_key_here
MODELSCOPE_ENDPOINT=https://api.modelscope.cn/v1

# Browser Configuration
CHROME_BINARY_PATH=/usr/bin/google-chrome

# Application Settings
DEBUG=false
LOG_LEVEL=INFO

# Skip LLM API Key Verification (for testing)
SKIP_LLM_API_KEY_VERIFICATION=false

